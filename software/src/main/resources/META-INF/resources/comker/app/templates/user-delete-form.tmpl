<form action="#" id="deleteUser" class="form-horizontal user-x-form">
    <div>
        <p>Are you sure you want to delete the user "<%=username%>" (email:<%=email%>)?</p>
        <ul>
            <li><button id="submit-delete-user" class="btn btn-primary" data-i18n="common:button.submit.label">Submit</button></li>
            <li><button id="cancel-delete-user" class="btn btn-primary" data-i18n="common:button.cancel.label">Cancel</button></li>
        </ul>
    </div>
</form>