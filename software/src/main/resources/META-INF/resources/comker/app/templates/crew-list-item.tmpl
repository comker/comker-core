<div class="crew-container">
    <div class="crew-overview">
        <ul>
            <li><span class="item-label" data-i18n="crew:crew.form.name">Name</span>: <%= name %></li>
            <li><span class="item-label" data-i18n="crew:crew.form.description">Description</span>: <%= description %></li>
        </ul>
        <button class="crew-button delete btn btn-danger btn-small" data-i18n="common:button.delete.label">Delete</button>
        <button class="crew-button update btn btn-info btn-small" data-i18n="common:button.update.label">Update</button>
        <button class="crew-button detail btn btn-info btn-small" data-i18n="common:button.detail.label">Detail</button>
    </div>
    <div class="crew-dialog">
        <div class="crew-message"></div>
        <div class="crew-content"></div>
    </div>
</div>