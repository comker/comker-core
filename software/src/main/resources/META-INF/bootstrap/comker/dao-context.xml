<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
            http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="comkerUserDao" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>net.cokkee.comker.dao.ComkerUserDao</value>
        </property>
        <property name="interceptorNames">
            <list>
            </list>
        </property>
        <property name="target" ref="comkerUserDaoHibernate" />
    </bean>

    <bean id="comkerUserDaoHibernate" class="net.cokkee.comker.dao.ComkerUserDao$Hibernate">
        <property name="sessionFactory" ref="comkerSessionFactory"/>
        <property name="crewDao" ref="comkerCrewDaoHibernate"/>
    </bean>


    <bean id="comkerSpotDao" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>net.cokkee.comker.dao.ComkerSpotDao</value>
        </property>
        <property name="interceptorNames">
            <list>
            </list>
        </property>
        <property name="target" ref="comkerSpotDaoHibernate" />
    </bean>

    <bean id="comkerSpotDaoHibernate" class="net.cokkee.comker.dao.ComkerSpotDao$Hibernate">
        <property name="sessionFactory" ref="comkerSessionFactory"/>
    </bean>


    <bean id="comkerCrewDao" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>net.cokkee.comker.dao.ComkerCrewDao</value>
        </property>
        <property name="interceptorNames">
            <list>
            </list>
        </property>
        <property name="target" ref="comkerCrewDaoHibernate" />
    </bean>

    <bean id="comkerCrewDaoHibernate" class="net.cokkee.comker.dao.ComkerCrewDao$Hibernate">
        <property name="sessionFactory" ref="comkerSessionFactory"/>
        <property name="roleDao" ref="comkerRoleDaoHibernate"/>
    </bean>


    <bean id="comkerRoleDao" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>net.cokkee.comker.dao.ComkerRoleDao</value>
        </property>
        <property name="interceptorNames">
            <list>
            </list>
        </property>
        <property name="target" ref="comkerRoleDaoHibernate" />
    </bean>

    <bean id="comkerRoleDaoHibernate" class="net.cokkee.comker.dao.ComkerRoleDao$Hibernate">
        <property name="sessionFactory" ref="comkerSessionFactory"/>
    </bean>


    <bean id="comkerPermissionDao" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>net.cokkee.comker.dao.ComkerPermissionDao</value>
        </property>
        <property name="interceptorNames">
            <list>
            </list>
        </property>
        <property name="target" ref="comkerPermissionDaoHibernate" />
    </bean>

    <bean id="comkerPermissionDaoHibernate" class="net.cokkee.comker.dao.ComkerPermissionDao$Hibernate">
        <property name="sessionFactory" ref="comkerSessionFactory"/>
    </bean>


    <bean id="comkerNavbarNodeDao" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>net.cokkee.comker.dao.ComkerNavbarNodeDao</value>
        </property>
        <property name="interceptorNames">
            <list>
            </list>
        </property>
        <property name="target" ref="comkerNavbarNodeDaoHibernate" />
    </bean>

    <bean id="comkerNavbarNodeDaoHibernate" class="net.cokkee.comker.dao.ComkerNavbarNodeDao$Hibernate">
        <property name="sessionFactory" ref="comkerSessionFactory"/>
    </bean>

</beans>
