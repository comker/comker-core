<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
            http://www.springframework.org/schema/beans 
            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="comkerQuestionResource"
            class="net.cokkee.comker.api.ComkerQuestionResource">
        <property name="storageManager" ref="comkerStorageManager" />
    </bean>

    <bean id="comkerStorageManager"
            class="net.cokkee.comker.service.impl.ComkerStorageManagerImpl" />

</beans>
